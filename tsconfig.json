{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "types": [
      "astro/env"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "database:functions": [
        "supabase/database.functions"
      ],
      "database:tables": [
        "util/types/suparows"
      ],
      "database:types": [
        "util/types/suparesponse"
      ],
      "database:client": [
        "supabase/database.client"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@components/*": [
        "components/*"
      ]
    }
  }
}